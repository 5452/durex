  <script>
		 jQuery(function(){

		  var len  = jQuery("#slider ul li").length;
		  var index = 0;
		  var adTimer;

		  jQuery('#slider').hover(function(){
	       clearInterval(adTimer);
		   },function(){
			adTimer = setInterval(function(){
	          index++;
			  if(index==len){index=0;}
  		      showImg(index) 			
 			 } , 2000);
 		  }).trigger("mouseleave");
		  
		  jQuery('.leftBtn').click(function(){
			if(index==len-1){
			   index=-1;
			}
			 index++;
			 showImg(index);
			 
 		
		})
		jQuery('.rightBtn').click(function(){
		   if(index==0){
			   index=len;
			 }
			 index--;
			 showImg(index);
			 

		})
		})
		
